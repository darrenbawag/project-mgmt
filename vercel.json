{	
	"version": 2,
	"builds": [
		{
			"src": "server/index.js",
			"use": "@vercel/node"
		},
		{
			"src": "client/build/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/graphql",
			"dest": "server/index.js"
		},
		{
			"src": "/",
			"dest": "client/build/index.html"
		},
		{
			"src": "/(.+)",
			"dest": "client/build/$1"
		},
		{
			"src": "/projects/(.+)",
			"dest": "client/build/index.html"
		}
	]
}
